@model BussinesEntities.PromocionesOtorgadasUsuario

@{
    ViewBag.Title = "Obtener Promocion";
}

<h2>OBTUVISTE LA PROMOCION!</h2>

<br />

<br />
    <div>
        <h4>Detalles</h4>
        <hr />
        <dl class="dl-horizontal">

            <dt>
                Negocio:
            </dt>

            <dd>
                @Html.DisplayFor(model => model.NEGOCIO)
            </dd>

            <dt>
                Promocion: 
            </dt>

            <dd>
                @Html.DisplayFor(model => model.PROMOCION)
            </dd>
                   
            <dt>
                Obtenida:
            </dt>

            <dd>
                @Html.DisplayFor(model => model.OBTENIDA)
            </dd>

            <dt>
                Vence:
            </dt>

            <dd>
                @Html.DisplayFor(model => model.VENCE)
            </dd>

            <dt>
                Codigo:
            </dt>

            <dd>
                @Html.DisplayFor(model => model.CODIGO)
            </dd>

        </dl>
    </div>
    <p>
        @{
            int idUsuario = ViewBag.IdUsuario;
        }
        <button style="font-size:15px; padding: 5px; width:30px" onclick="window.location = '@Url.Action("PromocionesUsuario", "Promociones", new { idUsuario = idUsuario })'" class="btn btn-xs btn-blue tooltips" data-placement="top" data-original-title="Editar">IR A TUS PROMOCIONES</button>
    </p>
